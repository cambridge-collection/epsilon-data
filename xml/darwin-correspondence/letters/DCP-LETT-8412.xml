                <term type="scientific_terms" ref="#">flowers and buds</term>
                <term type="scientific_terms" ref="#">plant physiology</term>
                <term type="scientific_terms" ref="#">structural characters</term>
                <term type="scientific_terms" ref="#">theory (including philosophy)</term>
                <term type="scientific_terms" ref="#">wind and weather</term>
            </keywords>
        </textClass>
