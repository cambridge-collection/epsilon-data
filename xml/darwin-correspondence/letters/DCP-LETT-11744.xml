                <term type="scientific_terms" ref="#">humanity</term>
                <term type="scientific_terms" ref="#">selection</term>
                <term type="scientific_terms" ref="#">social behaviour</term>
                <term type="scientific_terms" ref="#">theory (including philosophy)</term>
            </keywords>
        </textClass>
