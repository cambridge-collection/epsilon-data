                <term type="scientific_terms" ref="#">flora</term>
                <term type="scientific_terms" ref="#">highness and lowness</term>
                <term type="scientific_terms" ref="#">struggle for existence</term>
                <term type="scientific_terms" ref="#">systematics</term>
                <term type="scientific_terms" ref="#">theory (including philosophy)</term>
            </keywords>
        </textClass>
