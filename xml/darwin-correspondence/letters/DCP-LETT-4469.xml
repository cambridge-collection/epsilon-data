                <term type="people" ref="#subject_2486">Huxley, T. H.</term>
                <term type="people" ref="#subject_2932">Lindley, John</term>
                <term type="people" ref="#subject_4258">Scott, John</term>
                <term type="people" ref="#subject_4839">Tyndall, John</term>
                <term type="scientific_terms" ref="#">negative attitude/assessment</term>
            </keywords>
        </textClass>
