                <term type="scientific_terms" ref="#">fauna</term>
                <term type="scientific_terms" ref="#">geographical distribution</term>
                <term type="scientific_terms" ref="#">negative attitude/assessment</term>
                <term type="scientific_terms" ref="#">systematics</term>
